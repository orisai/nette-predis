{
	"name": "orisai/nette-predis",
	"description": "Predis integration into Nette",
	"keywords": [
		"nette",
		"orisai",
		"redis",
		"predis"
	],
	"type": "library",
	"license": "MPL-2.0",
	"homepage": "https://github.com/orisai/nette-predis",
	"authors": [
		{
			"name": "Marek BartoÅ¡",
			"homepage": "https://github.com/mabar"
		}
	],
	"require": {
		"php": ">=7.4.0 <8.2.0",
		"nette/di": "^3.0.0",
		"orisai/exceptions": "^1.0.0",
		"predis/predis": "^1.1.6",
		"symfony/lock": "^5.2.0"
	},
	"require-dev": {
		"infection/infection": "^0.20.0",
		"nette/http": "^3.0.0",
		"orisai/coding-standard": "^1.0.0",
		"orisai/nette-di": "^1.0.0",
		"pepakriz/phpstan-exception-rules": "^0.11.0",
		"phpstan/extension-installer": "^1.0.0",
		"phpstan/phpstan": "^0.12.0",
		"phpstan/phpstan-deprecation-rules": "^0.12.0",
		"phpstan/phpstan-nette": "^0.12.0",
		"phpstan/phpstan-phpunit": "^0.12.0",
		"phpstan/phpstan-strict-rules": "^0.12.0",
		"phpunit/phpunit": "^9.3.0",
		"staabm/annotate-pull-request-from-checkstyle": "^1.1.0"
	},
	"autoload": {
		"psr-4": {
			"OriNette\\Predis\\": "src"
		}
	},
	"autoload-dev": {
	  "psr-4": {
		"Tests\\OriNette\\Predis\\": "tests"
	  }
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"config": {
		"sort-packages": true
	}
}
